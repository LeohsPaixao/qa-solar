{
  "name": "dashboard",
  "version": "1.0.0",
  "description": "Dashboard for QA Solar Project",
  "author": "Leonardo Paixao <leonardohspaixao@gmail.com>",
  "license": "MIT",
  "type": "module",
  "private": true,
  "scripts": {
    "dev": "vite --host 127.0.0.1",
    "start": "yarn build && yarn dev",
    "build": "rimraf dist && vue-tsc -b && vite build",
    "lint": "eslint --fix \"src/**/*.{js,ts,vue}\"",
    "format": "prettier --write \"src/**/*.{js,ts,vue}\""
  },
  "dependencies": {
    "chart.js": "4.5.1",
    "dotenv": "16.4.5",
    "pinia": "3.0.4",
    "prettier": "3.3.3",
    "vue": "3.5.12",
    "vue-chartjs": "5.3.3",
    "vue-router": "4.4.5"
  },
  "devDependencies": {
    "@tsconfig/node22": "22.0.5",
    "@types/node": "22.19.1",
    "@typescript-eslint/eslint-plugin": "8.34.0",
    "@typescript-eslint/parser": "8.34.0",
    "@vitejs/plugin-vue": "5.1.4",
    "@vue/eslint-config-prettier": "10.1.0",
    "@vue/eslint-config-typescript": "14.5.0",
    "@vue/tsconfig": "0.8.1",
    "eslint": "9.20.0",
    "eslint-plugin-vue": "10.2.0",
    "rimraf": "6.0.1",
    "typescript": "5.8.3",
    "vite": "5.4.10",
    "vue-eslint-parser": "10.1.3",
    "vue-tsc": "3.1.4"
  }
}
