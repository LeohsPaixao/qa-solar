{
  "name": "frontend",
  "version": "2.0.3",
  "description": "frontend for QA Solar Project",
  "author": "Leonardo Paixao <leonardohspaixao@gmail.com>",
  "license": "MIT",
  "type": "module",
  "private": true,
  "scripts": {
    "dev": "rimraf dist && vite --host 127.0.0.1",
    "build": "rimraf dist && vite build",
    "preview": "vite preview",
    "type-check": "vue-tsc --noEmit",
    "test": "vitest run",
    "test:coverage": "vitest run --coverage",
    "lint": "eslint --fix \"src/**/*.{js,ts,vue}\"",
    "format": "prettier --write \"src/**/*.{js,ts,vue}\""
  },
  "dependencies": {
    "@jamescoyle/vue-icon": "0.1.2",
    "@mdi/js": "7.4.47",
    "@tanstack/vue-query": "5.60.6",
    "@vee-validate/yup": "4.15.1",
    "axios": "1.8.3",
    "dotenv": "16.4.5",
    "prettier": "3.3.3",
    "vee-validate": "4.15.1",
    "vue": "3.5.12",
    "vue-router": "4.4.5",
    "vue3-toastify": "0.2.8",
    "yup": "1.7.1"
  },
  "devDependencies": {
    "@tsconfig/node22": "22.0.5",
    "@types/node": "22.19.1",
    "@typescript-eslint/eslint-plugin": "8.34.0",
    "@typescript-eslint/parser": "8.34.0",
    "@vitejs/plugin-vue": "5.1.4",
    "@vitest/coverage-v8": "3.2.4",
    "@vue/eslint-config-prettier": "10.1.0",
    "@vue/eslint-config-typescript": "14.5.0",
    "@vue/test-utils": "2.4.6",
    "@vue/tsconfig": "0.8.1",
    "c8": "10.1.3",
    "eslint": "9.20.0",
    "eslint-plugin-vue": "10.2.0",
    "jsdom": "27.0.0",
    "npm-run-all2": "7.0.1",
    "postcss": "8.4.31",
    "rimraf": "6.0.1",
    "vite": "5.4.10",
    "vite-plugin-istanbul": "6.0.2",
    "vite-plugin-vue-devtools": "7.5.4",
    "vitest": "3.2.4",
    "vue-eslint-parser": "10.1.3",
    "vue-tsc": "3.1.4",
    "typescript": "5.8.3"
  }
}
