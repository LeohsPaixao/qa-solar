    "@nestjs/passport": "11.0.5",
    "@nestjs/platform-express": "11.1.3",
    "@nestjs/swagger": "11.2.0",
    "@prisma/client": "5.22.0",
    "@types/bcryptjs": "3.0.0",
    "@types/express": "5.0.3",
    "@types/node": "22.16.0",
    "@types/passport-jwt": "4.0.1",
    "bcryptjs": "3.0.2",
    "class-transformer": "0.5.1",
