{
  "name": "performance-tests",
  "version": "1.3.0",
  "description": "Testes de performance para o QA Solar",
  "main": "dist/index.js",
  "author": "QA Solar",
  "private": true,
  "license": "MIT",
  "scripts": {
    "build:performance": "rimraf dist && ts-node build.ts",
    "test:performance": "yarn build:performance && k6 run --out web-dashboard dist/index.js",
    "k6:report": "ts-node generate-report.ts",
    "format": "prettier --write ."
  },
  "keywords": [
    "k6",
    "performance"
  ],
  "dependencies": {
    "k6-html-reporter": "1.0.5",
    "rimraf": "6.0.1"
  },
  "devDependencies": {
    "@types/k6": "1.0.2",
    "@types/node": "22.16.0",
    "@typescript-eslint/eslint-plugin": "8.34.0",
    "@typescript-eslint/parser": "8.34.0",
    "esbuild": "0.25.5",
    "eslint": "9.20.0",
    "prettier": "3.3.3",
    "ts-node": "10.9.2",
    "typescript": "5.8.3"
  }
}