{
  "compilerOptions": {
    "baseUrl": ".",
    "esModuleInterop": true,
    "moduleResolution": "Node",
    "target": "es6",
    "lib": [
      "es6",
      "dom"
    ],
    "strict": true,
    "strictNullChecks": true,
    "skipLibCheck": true,
    "types": [
      "cypress",
      "node"
    ],
    "paths": {
      "@/*": [
        "./tests/e2e/*"
      ],
    }
  },
  "include": [
    "tests",
  ],
  "exclude": [
    "node_modules",
  ]
}